Options -Indexes

# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.php$ - [L]

# Multisite config
RewriteRule ^(.+/)?(wp-(content|admin|includes).*) wp/$2 [L]
RewriteRule ^([_0â€“9a-zA-Z-]+/)?(.*\.php)$ wp/$2 [L]

RewriteCond %{REQUEST_FILENAME} !-f [OR]
